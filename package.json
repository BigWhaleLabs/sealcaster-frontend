{
  "name": "sealcaster-frontend",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "yarn generate-css-types && vite",
    "build": "yarn generate-css-types && cross-env NODE_ENV=production tsc && vite build",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 --ext ts,tsx,json src",
    "generate-css-types": "tailwindcss-classnames -o src/classnames/tailwind.ts && prettier --write src/classnames/tailwind.ts",
    "prestart": "yarn generate-css-types",
    "prebuild": "yarn generate-css-types"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@big-whale-labs/constants": "^0.1.29",
    "@big-whale-labs/frontend-utils": "^0.0.8",
    "@big-whale-labs/seal-cred-posts-contract": "^0.0.13",
    "@big-whale-labs/stores": "^0.1.16",
    "@coinbase/wallet-sdk": "^3.5.2",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@material-tailwind/react": "^1.2.4",
    "@opengsn/provider": "^3.0.0-beta.1",
    "@vitejs/plugin-react": "^2.0.1",
    "@walletconnect/web3-provider": "^1.8.0",
    "assert-browserify": "^2.0.0",
    "dayjs": "^1.11.5",
    "entropy-string": "^4.2.0",
    "envalid": "^7.3.1",
    "eth-rpc-errors": "^4.0.3",
    "ethers": "^5.7.0",
    "ffjavascript": "^0.2.55",
    "preact": "^10.10.6",
    "react-infinite-scroll-component": "^6.1.0",
    "react-intersection-observer": "^9.4.0",
    "react-textarea-autosize": "^8.3.4",
    "react-toastify": "^9.0.8",
    "valtio": "^1.7.0",
    "web3-providers-http": "^1.7.5",
    "web3modal": "^1.9.9",
    "wouter": "^2.8.0-alpha.2"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@preact/preset-vite": "^2.3.0",
    "@rollup/plugin-inject": "^4.0.4",
    "@typechain/ethers-v5": "^10.1.0",
    "@types/entropy-string": "^4.2.0",
    "@types/node": "^18.7.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-react": "^2.0.1",
    "autoprefixer": "^10.4.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-relative-import-paths": "^1.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
