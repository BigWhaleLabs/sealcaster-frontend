    "assert-browserify": "^2.0.0",
    "dayjs": "^1.11.5",
    "entropy-string": "^4.2.0",
    "envalid": "^7.3.1",
    "eth-rpc-errors": "^4.0.3",
    "ethers": "^5.7.1",
    "ffjavascript": "^0.2.56",
    "preact": "^10.11.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-parallax-tilt": "^1.7.68",
    "react-textarea-autosize": "^8.3.4",
    "react-toastify": "^9.0.8",
    "valtio": "^1.7.0",
    "web3modal": "^1.9.9",
    "wouter": "^2.8.0-alpha.2"
  },
  "devDependencies": {
