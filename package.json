  "resolutions": {
    "@babel/core": "^7.20.2",
    "@opengsn/provider/@opengsn/common": "patch:@opengsn/common@npm%3A3.0.0-beta.3#./.yarn/patches/@opengsn-common-npm-3.0.0-beta.3-79cb41f503.patch",
    "@opengsn/provider/@opengsn/contracts": "3.0.0-beta.3"
  }
